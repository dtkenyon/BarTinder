<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.ntunick19.bartinder.MainActivity"
    tools:showIn="@layout/activity_main"
    android:background="#ffffff">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Deutschland Destructors"
        android:id="@+id/textView"
        android:layout_marginBottom="45dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Start"
        android:id="@+id/start_button"
        android:onClick="startClick"
        android:layout_centerVertical="true"
        android:layout_centerHorizontal="true"
        android:textSize="26dp"
        android:textAppearance="@android:style/TextAppearance.Holo"
        style="@style/Base.TextAppearance.AppCompat.Widget.Button" />

    <CheckBox
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="I am 21 years of age or older"
        android:id="@+id/checkBox"
        android:layout_marginTop="61dp"
        android:checked="false"
        android:layout_below="@+id/start_button"
        android:layout_centerHorizontal="true" />

</RelativeLayout>
